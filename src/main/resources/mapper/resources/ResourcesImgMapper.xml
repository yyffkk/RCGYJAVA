<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.aku.dao.resources.ResourcesImgDao">
    <select id="findGoodsImgByDateId" parameterType="com.aku.model.resources.ResourcesImg" resultType="com.aku.vo.resources.VoResourcesImg">
        select * from resources_img
        where table_name = #{tableName}
        and date_id = #{dateId}
        and type_name = #{typeName}
        order by sort
    </select>
</mapper>